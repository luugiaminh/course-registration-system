<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class catalog="CRS"
           name = "pojo.HocKi" table="HOCKI">
        <id name="id" type="integer">
            <column name="id" not-null="true"/>
            <generator class="identity"/>
        </id>
        <property name = "tenHK" type = "string">
                  <column length="3" name="TenHK"/>
        </property>
        <property name = "nam" type = "integer">
                  <column name="Nam"/>
        </property>
        <property name = "ngayBD" type = "string">
                  <column length="10" name="NgayBD"/>
        </property>
        <property name = "ngayKT" type = "string">
                  <column length="10" name="NgayKT"/>
        </property>
    </class>
</hibernate-mapping>
